<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" style="-fx-background-color: linear-gradient(#D9D9D9, #F2F2F2);" xmlns="http://javafx.com/javafx/14" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@menustyle.css">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="94.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="94.0" prefWidth="600.0" text="GooseGame" textAlignment="CENTER" textFill="#262626" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <font>
                        <Font name="Candara Bold" size="31.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <HBox alignment="CENTER" prefHeight="200.0" prefWidth="600.0" spacing="50.0">
               <children>
                  <Button fx:id="playButton" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" style="-fx-pref-width: 120; -fx-pref-height: 50;"  text="Play" textAlignment="CENTER" textFill="#f2f2f2">
                     <font>
                        <Font name="Candara Bold" size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="howToPlayButton" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-pref-width: 120; -fx-pref-height: 50;"  text="Credits" textAlignment="CENTER" textFill="#f2f2f2">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="exitButton" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-pref-width: 120; -fx-pref-height: 50;"  text="Exit" textAlignment="CENTER" textFill="#f2f2f2">
                     <font>
                        <Font name="Candara" size="16.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
